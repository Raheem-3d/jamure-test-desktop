{
  "name": "jamurechat",
  "version": "1.1.2",
  "description": "JamureChat - Team Communication App",
  "author": "3D Power",
  "private": true,
  "main": "main.js",
  "build": {
    "appId": "com.3dpower.chatapp",
    "productName": "JamureChat",
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "files": [
      "main.js",
      "server.js",
      "package.json",
      "preload.js",
      ".next/static/**/*",
      ".next/server/**/*",
      ".next/BUILD_ID",
      ".next/build-manifest.json",
      ".next/prerender-manifest.json",
      ".next/routes-manifest.json",
      ".next/required-server-files.json",
      "!.next/cache/**/*",
      "!.next/standalone/**/*",
      "public/**/*",
      ".env"
    ],
    "asarUnpack": [
      "node_modules/sqlite3/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "Raheem-3d",
        "repo": "jamure-test-chat"
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": "x64"
        }
      ],
      "icon": "public/Desktopicon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Jamure Chat",
      "installerIcon": "public/Desktopicon.ico",
      "uninstallerIcon": "public/Desktopicon.ico",
      "installerHeaderIcon": "public/Desktopicon.ico",
      "differentialPackage": false,
      "deleteAppDataOnUninstall": false
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "scripts": {
    "dev": "pnpm exec next dev",
    "build": "pnpm exec next build",
    "start": "pnpm exec next start",
    "test": "pnpm vitest run",
    "dist:win": "pnpm exec electron-builder --win nsis",
    "dist": "pnpm exec electron-builder",
    "optimize-build": "node scripts/optimize-build.js",
    "electron": "concurrently \"cross-env BROWSER=none pnpm exec next dev\" \"wait-on http://localhost:3000 && pnpm exec electron ./main.js\"",
    "electron:prod": "pnpm run build && concurrently \"cross-env BROWSER=none pnpm exec next start\" \"wait-on http://localhost:3000 && pnpm exec electron ./main.js\"",
    "release": "pnpm exec electron-builder --publish=always",
    "migrate:subs": "node scripts/migrate-subscriptions.js"
  },
  "dependencies": {
    "@auth/core": "latest",
    "@emotion/is-prop-valid": "latest",
    "@hello-pangea/dnd": "^18.0.1",
    "@hookform/resolvers": "^3.9.1",
    "@internationalized/date": "^3.8.2",
    "@mantine/core": "^8.2.1",
    "@mantine/notifications": "^8.2.1",
    "@neondatabase/serverless": "^1.0.1",
    "@next-auth/prisma-adapter": "latest",
    "@prisma/client": "^6.8.2",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "latest",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
